# NextAuth Configuration
NEXTAUTH_URL=https://your-domain.vercel.app
NEXTAUTH_SECRET=your-nextauth-secret-key-here
AUTH_SECRET=your-nextauth-secret-key-here

# Google OAuth (optional - comment out if not using)
AUTH_GOOGLE_ID=your-google-client-id
AUTH_GOOGLE_SECRET=your-google-client-secret

# Facebook OAuth (optional - comment out if not using)
AUTH_FACEBOOK_ID=your-facebook-app-id
AUTH_FACEBOOK_SECRET=your-facebook-app-secret

# Channel Talk Configuration (optional)
NEXT_PUBLIC_CHANNEL_TALK_PLUGIN_KEY=your-channel-talk-plugin-key
CHANNEL_TALK_ACCESS_SECRET=your-channel-talk-access-secret

# Site Configuration
NEXT_PUBLIC_SITE_URL=https://your-domain.vercel.app
SITE_URL=https://your-domain.vercel.app